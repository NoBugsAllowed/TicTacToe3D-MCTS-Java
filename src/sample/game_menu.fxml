<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?scenebuilder-classpath-element ?>
<BorderPane prefHeight="212.0" prefWidth="151.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Main">
    <padding>
        <Insets left="20" right="20"/>
    </padding>
    <center>
        <VBox alignment="CENTER" spacing="30">
            <Button fx:id="btnPlayAgain" prefWidth="300"
                    style="-fx-background-image: url(&quot;sample/icons/playAgain.png&quot;);" styleClass="menuButton"
                    text="Play again"/>
            <Button fx:id="btnStartMenu" prefWidth="300"
                    style="-fx-background-image: url(&quot;sample/icons/backToMenu.png&quot;);" styleClass="menuButton"
                    text="Main menu"/>
            <Button fx:id="btnExit" prefWidth="300"
                    style="-fx-background-image: url(&quot;sample/icons/exit.png&quot;);" styleClass="menuButton"
                    text="Exit"/>
        </VBox>
    </center>
</BorderPane>
